{
  "name": "@typescript-eslint/typescript-estree",
  "version": "2.5.0",
  "main": "dist/index.js",
  "private": true,
  "files": [
    "dist"
  ],
  "license": "MIT",
  "engine": {
    "node": ">=6"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "rollup --config && node ./scripts/fix-code.js",
    "build:package-json": "node ./scripts/sync-version.js",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.7.0",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.4",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.6",
    "@fisker/prettier-config": "1.0.21",
    "@types/mocha": "5.2.7",
    "@typescript-eslint/experimental-utils": "2.6.1",
    "@typescript-eslint/typescript-estree": "2.6.0",
    "core-js": "3.3.6",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.6.0",
    "husky": "3.0.9",
    "jest": "24.9.0",
    "lint-staged": "9.4.2",
    "markdownlint-cli": "0.19.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "read-pkg-up": "7.0.0",
    "regenerator-runtime": "0.13.3",
    "rollup": "^1.26.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "ts-jest": "24.1.0",
    "typescript": "3.7.0-dev.20191021",
    "write-pkg": "4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false
}
