{
  "name": "@typescript-eslint/typescript-estree",
  "version": "2.4.0",
  "main": "index.js",
  "private": true,
  "files": [
    "dist"
  ],
  "license": "MIT",
  "engine": {
    "node": ">=6"
  },
  "scripts": {
    "build": "run-p build:*",
    "build:js": "webpack",
    "build:package-json": "node ./scripts/sync-version.js",
    "format": "run-p format:*",
    "format:eslint": "yarn lint:eslint --fix",
    "format:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --write",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint \"**/*.{js,mjs,vue}\"",
    "lint:markdown": "markdownlint \"**/*.md\" --ignore \"**/node_modules/**\"",
    "lint:prettier": "prettier \"**/*.{css,html,js,json,less,md,scss,ts,vue,yaml,yml}\" --check"
  },
  "devDependencies": {
    "@babel/core": "7.6.4",
    "@babel/preset-env": "7.6.3",
    "@commitlint/cli": "8.2.0",
    "@fisker/commitlint-config": "1.1.3",
    "@fisker/eslint-config": "1.3.4",
    "@fisker/husky-config": "1.1.3",
    "@fisker/lint-staged-config": "1.0.5",
    "@fisker/prettier-config": "1.0.21",
    "@typescript-eslint/typescript-estree": "2.4.0",
    "babel-loader": "8.0.6",
    "builtin-modules": "3.1.0",
    "core-js": "3",
    "cz-conventional-changelog": "3.0.2",
    "eslint": "6.5.1",
    "husky": "3.0.9",
    "lint-staged": "9.4.2",
    "markdownlint-cli": "0.18.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "read-pkg-up": "7.0.0",
    "regenerator-runtime": "0.13.3",
    "typescript": "3.7.0-dev.20191017",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.9",
    "write-pkg": "4.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": {
    "name": "fisker Cheung",
    "email": "lionkay@gmail.com",
    "url": "https://www.fiskercheung.com/"
  },
  "sideEffects": false
}
